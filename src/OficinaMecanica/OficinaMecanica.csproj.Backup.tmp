    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>fa3b4b7c-f620-47e1-b1bf-aabac0fffb95</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
